<?xml version="1.0" encoding="UTF-8"?>
<xml>
	<config>
		<serviceUrl>@SERVICE_URL@</serviceUrl>
		<partnerId>99</partnerId>
		<clientTag>Stand alone php 1.0.0</clientTag>
		<curlTimeout>30</curlTimeout>
		<userAgent>Stand alone php 1.0.0</userAgent>
	</config>
	<session>
		<partnerId>99</partnerId>
		<secret>@TEMPLATE_PARTNER_ADMIN_SECRET@</secret>
		<userId></userId>
		<sessionType>2</sessionType>
		<expiry>86400</expiry>
		<privileges></privileges> 
	</session>
	<request service="uiConf" action="add">
		<uiConf objectType="KalturaUiConf">
			<objType>1</objType>
			<name>Widescreen player - light skin</name>
			<width>400</width>
			<height>285</height>
			<swfUrl>/flash/kdp3/v3.5.21/kdp3.swf</swfUrl>
			<useCdn>1</useCdn>
			<tags>kdp3,player</tags>
			<creationMode>2</creationMode>
			<confFile path="ui_conf/widescreenLightskin.xml" />
			<confFileFeatures path="ui_conf/widescreenLightskin.features.xml" />
		</uiConf>
	</request>
	<request service="uiConf" action="add">
		<uiConf objectType="KalturaUiConf">
			<objType>1</objType>
			<name>Widescreen playlist player - dark skin, minimal</name>
			<width>740</width>
			<height>255</height>
			<swfUrl>/flash/kdp3/v3.5.21/kdp3.swf</swfUrl>
			<useCdn>1</useCdn>
			<tags>kdp3,playlist</tags>
			<creationMode>2</creationMode>
			<confFile path="ui_conf/widescreenPlaylistDark.xml" />
			<confFileFeatures path="ui_conf/widescreenPlaylistDark.features.xml" />
		</uiConf>
	</request>
	<request service="uiConf" action="add">
		<uiConf objectType="KalturaUiConf">
			<objType>1</objType>
			<name>Player with no share button</name>
			<width>400</width>
			<height>360</height>
			<swfUrl>/flash/kdp3/v3.5.21/kdp3.swf</swfUrl>
			<useCdn>1</useCdn>
			<tags>kdp3,player</tags>
			<creationMode>2</creationMode>
			<confFile path="ui_conf/playerNoShare.xml" />
			<confFileFeatures path="ui_conf/playerNoShare.features.xml" />
		</uiConf>
	</request>
	<request service="uiConf" action="add">
		<uiConf objectType="KalturaUiConf">
			<objType>1</objType>
			<name>Player with custom watermark</name>
			<width>400</width>
			<height>330</height>
			<swfUrl>/flash/kdp3/v3.5.21/kdp3.swf</swfUrl>
			<useCdn>1</useCdn>
			<tags>kdp3,player</tags>
			<creationMode>2</creationMode>
			<confFile path="ui_conf/WatermarkPlayer.xml" />
			<confFileFeatures path="ui_conf/WatermarkPlayer.features.xml" />
		</uiConf>
	</request>
	<request service="uiConf" action="add">
		<uiConf objectType="KalturaUiConf">
			<objType>1</objType>
			<name>Multiple playlists - dark skin</name>
			<width>740</width>
			<height>330</height>
			<swfUrl>/flash/kdp3/v3.5.21/kdp3.swf</swfUrl>
			<useCdn>1</useCdn>
			<tags>kdp3,playlist</tags>
			<creationMode>2</creationMode>
			<confFile path="ui_conf/MultiPlaylists.xml" />
			<confFileFeatures path="ui_conf/MultiPlaylists.features.xml" />
		</uiConf>
	</request>
</xml>